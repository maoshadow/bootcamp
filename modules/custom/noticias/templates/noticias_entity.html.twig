{#
/**
 * @file noticias_entity.html.twig
 * Default theme implementation to present Noticias data.
 *
 * This template is used when viewing Noticias pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_noticias_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('noticias_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
